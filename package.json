{
  "name": "markup",
  "version": "1.0.0",
  "description": "Write markdown with a WYSIWYG and get the source.",
  "main": "main.js",
  "jshintConfig": {
    "esversion": 6,
    "asi": true
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    }
  },
  "build": {
    "app-bundle-id": "markup",
    "app-category-type": "markdown.text.editor",
    "iconUrl": "res/markup-icon.png"
  },
  "dependencies": {
    "font-awesome": "^4.6.1",
    "github-markdown-css": "^2.2.1",
    "highlight.js": "^9.5.0",
    "remarkable": "^1.6.2",
    "to-markdown": "^3.0.0",
    "toolbarjs": "^2.4.17"
  },
  "keywords": [
    "markdown",
    "text",
    "desktop"
  ],
  "author": "Timur Uzel <timurt.uzel@gmail.com>",
  "homepage": "https://github.com/timurtu/markdown-text-editor#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timurtu/markup.git"
  },
  "bugs": {
    "url": "https://github.com/timurtu/markdown-text-editor/issues"
  },
  "license": "ISC",
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "del": "^2.2.1",
    "electron-prebuilt": "^1.2.5",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-eslint": "^3.0.0",
    "gulp-sass": "^2.3.2"
  }
}
